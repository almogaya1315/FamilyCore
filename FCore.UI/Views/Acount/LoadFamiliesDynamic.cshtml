@model ICollection<SelectListItem>

<div class="container" style="padding-left:0">
    <input type="text" id="relfam_droptext" style="display: block; line-height: inherit; height: 20px; margin-bottom: -1px; width:15.5%" placeholder="Search" oninput="LoadFamiliesDynamic()" />
    <select name="RelativeFamily" id="famenum_box" style="width:15.5%" onchange="FamilyListOnChange()">
        @{
            foreach (SelectListItem item in Model)
            {
                if (item.Value == "ph")
                {
                    <option hidden>@item.Text</option>
                }
                else
                {
                    <option>@item.Text</option>
                }
            }
        }
    </select>
    @Html.ValidationMessage("fam_required");
</div>

